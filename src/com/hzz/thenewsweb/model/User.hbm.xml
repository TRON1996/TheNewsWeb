<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.hzz.thenewsweb.model">

<class name="User" table="Table_User">

	<id column="id" name="id" type="integer"><generator class="native" /></id>
<!--	<property name="user" column="user" type="string" />-->
 
  <property column="name" name="name" type="string"/>
  <property column="sex" name="sex" type="string"/>
  <property column="phone" name="phone" type="string"/>
  <property column="password" name="password" type="string"/>
  <property column="photo" name="photo" type="string"/>
  <property column="selfinfo" name="selfinfo" type="string"/>

</class>
</hibernate-mapping>
